{
  "name": "@safeoutput/lib",
  "version": "0.2.0",
  "private": true,
  "exports": {
    "./client/*": "./src/client/*.ts",
    "./server/*": "./src/server/*.ts",
    "./shared/*": "./src/shared/*.ts"
  },
  "sideEffects": false,
  "browser": {
    "fs": false,
    "path": false,
    "crypto": false,
    "./server/*": false
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"**/*.ts\"",
    "check-types": "tsc --noEmit",
    "kysely:init": "kysely init",
    "kysely:codegen": "tsx src/scripts/pg-codegen.ts",
    "kysely:migrate": "kysely migrate latest",
    "test": "vitest run"
  },
  "devDependencies": {
    "@safeoutput/eslint-config": "workspace:*",
    "@safeoutput/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.5",
    "eslint": "^9.34.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "@ory/client": "^1.22.2",
    "fastify": "^5.6.0",
    "stripe": "^18.5.0"
  },
  "dependencies": {
    "@safeoutput/contracts": "workspace:*",
    "dotenv": "^17.2.2",
    "find-up": "^6.0.0",
    "kysely": "^0.28.7",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "pg-mem": "^3.0.5",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.5",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  }
}
