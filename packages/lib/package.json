{
  "name": "@safeoutput/lib",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./client/*": {
      "browser": "./src/client/*.tsx"
    },
    "./server/*": {
      "node": "./src/server/*.tsx"
    }
  },
  "browser": {
    "fs": false,
    "path": false,
    "crypto": false
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@safeoutput/eslint-config": "workspace:*",
    "@safeoutput/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.34.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "pg": "^8.16.3",
    "kysely": "^0.28.7"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "kysely": {
      "optional": true
    }
  },
  "dependencies": {
    "zod": "^4.1.11"
  }
}
